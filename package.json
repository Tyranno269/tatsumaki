{
  "name": "@tyranno269/tatsumaki",
  "version": "1.1.0",
  "description": "Automatically generate TypeSpec models from Rails database schema",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "tatsumaki": "dist/index.js"
  },
  "files": [
    "dist/**/*",
    "!dist/**/*.test.*",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "rails",
    "typespec",
    "openapi",
    "schema",
    "generator",
    "typescript",
    "api",
    "codegen"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint && npm run build",
    "postbuild": "chmod +x dist/index.js || true",
    "test": "vitest run",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tyranno269/tatsumaki.git"
  },
  "homepage": "https://github.com/Tyranno269/tatsumaki#readme",
  "bugs": {
    "url": "https://github.com/Tyranno269/tatsumaki/issues"
  },
  "author": "tyranno269",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/minimist": "^1.2.5",
    "@types/node": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-prettier": "^5.5.4",
    "lefthook": "^2.0.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.3.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "typescript-transform-paths": "^3.5.5",
    "vitest": "^4.0.7"
  },
  "dependencies": {
    "fast-glob": "^3.3.3",
    "minimist": "^1.2.8"
  }
}
